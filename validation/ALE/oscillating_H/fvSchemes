FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         backward;
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
    default Gauss linear;
    div(((1|A(U))*IBForce))      Gauss linear;
    div(IBForce)      Gauss linear;
    div(phi,U)      Gauss linear;
    div(U)      Gauss linear;
    div(phi_0,U_0)  Gauss linear;
}

laplacianSchemes
{
    default         Gauss linear orthogonal;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         orthogonal;
}

fluxRequired
{
    default no;
    p               ;
}


// ************************************************************************* //
