FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         none;
    ddt(U)          backward;
}

gradSchemes
{
    default       least squares;
    grad(p)       Gauss linear;
    grad(p_0)     Gauss linear;
    grad(U)       cellLimited leastSquares 1.;
}

divSchemes
{
    default         bounded Gauss linearUpwind grad(U);
}

laplacianSchemes
{
    default         Gauss linear corrected;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         corrected;
}


// ************************************************************************* //
